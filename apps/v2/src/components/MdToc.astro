---
import type { MarkdownHeading } from 'astro';
import { CollapsibleMdToc } from './collapsible-md-toc';
import MdTocSticky from './MdTocSticky.astro';

export interface Props {
  headings: Array<MarkdownHeading>;
}

const { headings } = Astro.props;
---

<CollapsibleMdToc headings={headings} className="xl:hidden" client:visible />
<MdTocSticky headings={headings} labelId="rightStickyToc" />
